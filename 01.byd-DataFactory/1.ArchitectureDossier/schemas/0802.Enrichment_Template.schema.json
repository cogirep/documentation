{
  "$schema": "Enrichment_Template_schema.json",
  "$id": "enrichment-template.schema.json",
  "type": "object",
  "title": "Enrichment Template Schema",
  "definitions": {
    "enrichment-template": {
        "properties": {
          "_id": {
            "$id": "#/properties/_id",
            "type": "string",
            "title": "The _id Schema",
            "default": "",
            "examples": [
              "1234"
            ],
            "pattern": "^(.*)$"
          },
          "createdAt": {
            "$id": "#/properties/createdAt",
            "type": "string",
            "title": "The Createdat Schema",
            "default": "",
            "examples": [
              "12154545"
            ],
            "pattern": "^(.*)$"
          },
          "revisionId": {
            "$id": "#/properties/revisionId",
            "type": "string",
            "title": "The Revisionid Schema",
            "default": "",
            "examples": [
              "515545"
            ],
            "pattern": "^(.*)$"
          },
          "label": {
            "$id": "#/properties/label",
            "type": "string",
            "title": "The Label Schema",
            "default": "",
            "examples": [
              "test"
            ],
            "pattern": "^(.*)$"
          },
          "status": {
            "$id": "#/properties/status",
            "type": "string",
            "title": "The Status Schema",
            "default": "",
            "examples": [
              "ACTIF"
            ],
            "pattern": "^(.*)$"
          },
          "templateSchema": {
            "$id": "#/properties/templateSchema",
            "type": "object",
            "title": "The Templateschema Schema",
            "required": [
              "key",
              "key2"
            ],
            "properties": {
              "key": {
                "$id": "#/properties/templateSchema/properties/key",
                "type": "object",
                "title": "The Key Schema",
                "required": [
                  "type"
                ],
                "properties": {
                  "type": {
                    "$id": "#/properties/templateSchema/properties/key/properties/type",
                    "type": "string",
                    "title": "The Type Schema",
                    "default": "",
                    "examples": [
                      "boolean"
                    ],
                    "pattern": "^(.*)$"
                  }
                }
              },
              "key2": {
                "$id": "#/properties/templateSchema/properties/key2",
                "type": "object",
                "title": "The Key2 Schema",
                "required": [
                  "type"
                ],
                "properties": {
                  "type": {
                    "$id": "#/properties/templateSchema/properties/key2/properties/type",
                    "type": "string",
                    "title": "The Type Schema",
                    "default": "",
                    "examples": [
                      "string"
                    ],
                    "pattern": "^(.*)$"
                  }
                }
              }
            }
          }
        },
        "required": [
          "_id",
          "createdAt",
          "revisionId",
          "label",
          "status",
          "templateSchema"
        ]
      }
    }
}
